extends ../templates/default

block content
  div(class='row')
    div(class='col-md-6 col-md-offset-3')
      div(class='panel panel-default')
        div(class='panel-heading')= 'Consultar Estatus'
        div(class='panel-body')
          form(action='http://localhost:3000/users/ePanel' method='post' autocomplete='off')
             div(class='form-group')
                    label(for="id")= "Id"
                    input(type='text' name='id' id='id' placeholder='Id' class='form-control')   
              button(type="submit" class='btn btn-default')= 'Buscar' 
              